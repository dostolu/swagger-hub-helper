{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "import/no-dynamic-require": 0, // As we need this for this particular proect
    "no-console": 0,
    "no-unused-expressions": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "array-callback-return": 0,
    "no-param-reassign": 0,
    "no-confusing-arrow": 0,
    "func-names": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "arrow-parens": 0
  }
}